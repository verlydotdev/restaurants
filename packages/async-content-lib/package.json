{
  "name": "@verlydotdev/async-content-lib",
  "type": "module",
  "version": "0.0.0",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.mjs",
      "require": "./dist/types.cjs"
    }
  },
  "files": [
    "dist",
    "*.d.ts"
  ],
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "scripts": {
    "build": "unbuild",
    "prepack": "unbuild",
    "dev": "vitest dev",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "start": "esno src/index.ts",
    "test": "vitest"
  },
  "devDependencies": {
    "esno": "^0.16.3",
    "typescript": "^4.8.3",
    "unbuild": "^1.1.1",
    "vite": "^4.0.4",
    "vitest": "^0.23.4"
  },
  "dependencies": {
    "consola": "^2.15.3",
    "pocketbase": "^0.10.0",
    "ufo": "^1.0.1"
  }
}
